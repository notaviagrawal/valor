# Google Maps API Key
# Get this from https://console.cloud.google.com/apis/credentials
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# NextAuth Configuration
# Generate with: npx auth secret
AUTH_SECRET=your_auth_secret_here

# HMAC Secret Key for wallet authentication
# Generate with: openssl rand -base64 32
HMAC_SECRET_KEY=your_hmac_secret_key_here

# Auth URL for NextAuth callbacks
# For local development: http://localhost:3000
# For production: your production domain
AUTH_URL=http://localhost:3000

# World ID Application ID
# Get this from the World ID developer portal
NEXT_PUBLIC_APP_ID=app_1234567890

# Worldchain / VAL transaction envs
WORLDCHAIN_RPC_URL=https://worldchain-mainnet.g.alchemy.com/public
NEXT_PUBLIC_VAL_TOKEN_ADDRESS=0xe9D770d3C03D3289EdA927Ed9d0A2a7c84186b6D
NEXT_PUBLIC_ESCROW_ADDRESS=0xYourEscrowContract
NEXT_PUBLIC_DISTRIBUTOR_ADDRESS=0xYourFiveValDistributor
NEXT_PUBLIC_REFUND_DISTRIBUTOR_ADDRESS=0xCc682D423ec8479Ccc8a33B89A560E68CC3430D2
ESCROW_ADMIN_PRIVATE_KEY=0xyourtestprivatekey
